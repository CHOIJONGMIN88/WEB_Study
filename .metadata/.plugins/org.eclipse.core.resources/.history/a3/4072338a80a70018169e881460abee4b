<mapper namespace="photo">

	<select id="photo_list" resultType="vo.PhotoVo">

	select * from photo order by idx desc
	
	</select>

 
 <insert id="photo_insert"   parameterType="vo.PhotoVo">

    insert into photo values(seq_visit_idx.nextVal,
    	                           #{ title },
    	                           #{ filename },
    	                           #{ pwd },
    	                           #{ ip }
    	                           )
    
    </insert>







</mapper>












